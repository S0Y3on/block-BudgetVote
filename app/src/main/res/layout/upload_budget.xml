                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:text="내용"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/upload_budget_expenditure"
                    android:layout_width="80dp"
                    android:layout_height="30dp"
                    android:text="금액"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/upload_budget_note"
                    android:layout_width="140dp"
                    android:layout_height="30dp"
                    android:text="비고"
                    android:textSize="18sp" />
