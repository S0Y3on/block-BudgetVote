<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Check_Execution_Vote_Result">

    <TextView
        android:id="@+id/vote_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="63dp"
        android:text="총 참여자 수 :"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/current"
        app:layout_constraintStart_toStartOf="@+id/disagree_result"
        app:layout_constraintTop_toBottomOf="@+id/disagree_result" />

    <Button
        android:id="@+id/back"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginTop="200dp"
        android:layout_marginBottom="69dp"
        android:text="뒤로가기"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/disagree_result"
        app:layout_constraintStart_toStartOf="@+id/disagree_result"
        app:layout_constraintTop_toBottomOf="@+id/disagree_result" />

    <TextView
        android:id="@+id/disagree_result"
        android:layout_width="197dp"
        android:layout_height="93dp"
        android:layout_marginStart="70dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="70dp"
        android:layout_marginRight="70dp"
        android:text="반대 : 000 %"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/agree_result" />

    <TextView
        android:id="@+id/result"
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_marginStart="70dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="70dp"
        android:layout_marginRight="70dp"
        android:text="투표 결과"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/agree_result"
        android:layout_width="196dp"
        android:layout_height="85dp"
        android:layout_marginStart="70dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="70dp"
        android:layout_marginRight="70dp"
        android:text="찬성 : 000 %"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/result" />

</androidx.constraintlayout.widget.ConstraintLayout>